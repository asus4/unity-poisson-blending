#pragma kernel CSMain

RWStructuredBuffer<int> buffer;


void recursive(int id)
{
    int depth = 0;
    while(depth < 2)
    {
        // buffer[id] = buffer[id] + buffer[id];
        buffer[id]++;
        depth++;
    }
}


[numthreads(4,4,1)]
void CSMain (uint3 tid : SV_DispatchThreadID)
{
    int id = tid.x + tid.y * 8;
    buffer[id] = id;
    recursive(id);
}